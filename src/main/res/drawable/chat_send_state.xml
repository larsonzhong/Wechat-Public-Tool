<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

	<item>
		<shape android:shape="rectangle">
			<padding
				android:bottom="1px"
				android:left="1px"
				android:right="1px"
				android:top="1px" />
			<solid android:color="#f1f1f1" android:width="1dp"/>
			<corners android:radius="15dip"/>
            <stroke android:width="1px" android:color="#b1b1b1"/>
		</shape>
	</item>

</layer-list>
